{
  "name": "webpack-es6-boilerplate",
  "version": "1.0.1",
  "author": "Dmitriy Yurov",
  "description": "A delightfully Webpack ES6 Boilerplate",
  "license": "MIT",
  "keywords": [
    "webpack",
    "boilerplate",
    "es6",
    "babel",
    "eslint",
    "jest",
    "sass"
  ],
  "main": "src/index.js",
  "scripts": {
    "start": "webpack-dev-server --config config/webpack.dev.js",
    "build": "webpack --env=prod --progress --profile --colors",
    "lint": "eslint ./src/**.js",
    "coverage": "jest --coverage --config=config/jest.config.js",
    "test": "jest --config=config/jest.config.js",
    "test:watch": "jest --watchAll"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.1",
    "babel-jest": "22.2.0",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "clean-webpack-plugin": "0.1.18",
    "css-loader": "^0.28.9",
    "eslint": "4.17.0",
    "eslint-loader": "1.9.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "2.30.1",
    "image-webpack-loader": "^4.0.0",
    "jest": "22.2.1",
    "node-sass": "4.7.2",
    "sass-loader": "6.0.6",
    "style-loader": "^0.20.1",
    "webpack": "3.10.0",
    "webpack-dev-server": "2.11.1",
    "webpack-merge": "4.1.1"
  },
  "dependencies": {
    "es6-promise": "^4.2.4",
    "normalize-scss": "^7.0.1"
  }
}
